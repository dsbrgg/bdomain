{
  "name": "bdomain-backend",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon dist/server.js",
    "start": "node dist/server.js",
    "build": "rollup -c && babel src/server -d dist && cp -r src/server/public dist/",
    "build:dev": "NODE_ENV=dev npm run build",
    "docker": "docker-compose up",
    "dev:push": "heroku container:push web -a bdomain-dev",
    "dev:release": "heroku container:release web -a bdomain-dev",
    "dev:logs": "heroku logs -a bdomain-dev --tail"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "config": "^3.3.1",
    "koa": "^2.12.0",
    "koa-router": "^9.0.1",
    "koa-send": "^5.0.0",
    "puppeteer": "^3.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.4",
    "@babel/core": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-commonjs": "^13.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^8.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "nodemon": "^2.0.4",
    "rollup": "^2.18.1",
    "rollup-plugin-livereload": "^1.3.0",
    "rollup-plugin-svelte": "^5.2.3",
    "rollup-plugin-terser": "^6.1.0",
    "svelte": "^3.23.2",
    "svelte-routing": "^1.4.2"
  }
}
