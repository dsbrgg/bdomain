{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "esmodules": true
        }
      }
    ]
  ],
  "plugins": [
    [
      "module-resolver", 
      {
        "root": ["./src/server"],
        "alias": {
          "test": "./test",
          "public": "./src/server/public"
        }
      }
    ]
  ]
}
